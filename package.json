{
  "name": "kdix-homepage",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev:front": "vite --config front/vite.config.ts",
    "dev:server": "vite-node --watch server/src/index.ts --config server/vite.config.ts",
    "dev": "npm-run-all --parallel dev:front dev:server",
    "build:front": "vite build --config front/vite.config.ts",
    "build:server": "vite build --config server/vite.config.ts",
    "build": "npm-run-all --parallel build:front build:server",
    "test:front": "vitest --config front/vitest.config.ts",
    "test:server": "vitest --config server/vitest.config.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-katex": "^3.1.0",
    "react-router-dom": "^7.8.0",
    "vite-node": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@fastify/cors": "^11.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-katex": "^3.0.4",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fastify": "^5.4.0",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
